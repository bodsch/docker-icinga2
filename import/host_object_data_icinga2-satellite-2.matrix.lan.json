{
  "templates": [ "satellite-host" ],
  "attrs": {
    "command_endpoint": "%FQDN%",
    "zone": "%FQDN%",
    "enable_notifications": true,
    "groups": ["icinga-satellites"],
    "max_check_attempts": "2",
    "check_interval": "30",
    "retry_interval": "10",
    "vars": {
      "os": "Docker",
      "remote_endpoint": "%FQDN%",
      "satellite": "true",
      "qa": "true",
      "date": "2019-02-13",
      "memory": "true",
      "file_age": {
        "/host/tmp/backup": {
          "warning_time": 76800,
          "critical_time": 128000
        }
      },
      "http_vhosts": {
        "/": {
          "http_uri": "/"
        }
      }
    }
  }
}
